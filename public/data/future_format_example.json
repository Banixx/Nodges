{
  "system": "Psychologisches Beziehungsnetzwerk - Universitätsteam",
  "metadata": {
    "created": "2025-01-27T21:00:00Z",
    "version": "1.0",
    "author": "Nodges Future System",
    "description": "Beispiel für ein psychologisches Beziehungsnetzwerk mit emotionalen Verbindungen zwischen Teammitgliedern"
  },
  "dataModel": {
    "entities": {
      "person": {
        "properties": {
          "age": {
            "type": "continuous",
            "range": [18, 70],
            "unit": "years"
          },
          "personality": {
            "type": "vector",
            "dimensions": ["openness", "conscientiousness", "extraversion", "agreeableness", "neuroticism"],
            "range": [0, 1]
          },
          "position": {
            "type": "spatial",
            "coordinates": ["x", "y", "z"]
          },
          "influence": {
            "type": "continuous",
            "range": [0, 1]
          },
          "department": {
            "type": "categorical",
            "values": ["IT", "Marketing", "Research", "Management", "Design"]
          },
          "experience": {
            "type": "continuous",
            "range": [0, 50],
            "unit": "years"
          }
        }
      }
    },
    "relationships": {
      "emotional": {
        "properties": {
          "trust": {
            "type": "continuous",
            "range": [0, 1]
          },
          "collaboration": {
            "type": "continuous",
            "range": [0, 1]
          },
          "conflict": {
            "type": "continuous",
            "range": [0, 1]
          },
          "communication_frequency": {
            "type": "continuous",
            "range": [0, 10],
            "unit": "interactions_per_week"
          },
          "relationship_duration": {
            "type": "temporal",
            "unit": "months"
          },
          "emotional_valence": {
            "type": "continuous",
            "range": [-1, 1]
          }
        }
      }
    }
  },
  "visualMappings": {
    "defaultPresets": {
      "person": {
        "position": {
          "source": "position",
          "function": "geographic"
        },
        "size": {
          "source": "influence",
          "function": "exponential",
          "range": [0.5, 3.0],
          "params": {
            "base": 2
          }
        },
        "color": {
          "source": "personality.extraversion",
          "function": "heatmap",
          "palette": "blue-red"
        },
        "geometry": {
          "source": "age",
          "function": "sphereComplexity",
          "params": {
            "minSegments": 6,
            "maxSegments": 24
          }
        },
        "glow": {
          "source": "experience",
          "function": "linear",
          "range": [0, 0.5]
        }
      },
      "emotional": {
        "thickness": {
          "source": "trust",
          "function": "exponential",
          "range": [0.05, 0.8],
          "params": {
            "base": 3
          }
        },
        "color": {
          "source": "emotional_valence",
          "function": "bipolar",
          "params": {
            "positive": "#ff69b4",
            "negative": "#8b0000"
          }
        },
        "curvature": {
          "source": "relationship_duration",
          "function": "logarithmic",
          "range": [0.1, 2.0]
        },
        "glow": {
          "source": "collaboration",
          "function": "pulse",
          "params": {
            "frequency": "heartbeat"
          }
        },
        "opacity": {
          "source": "communication_frequency",
          "function": "linear",
          "range": [0.3, 1.0]
        }
      }
    }
  },
